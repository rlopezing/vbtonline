<struts>

    <package name="Portafolio" namespace="/" extends="struts-default,json-default">

        <action name="Portafolio_*" method="{1}" class="ve.com.vbtonline.vista.action.portafolio.PortafolioAction">
            <result name="input">/jsp/portafolio/Portafolio.jsp</result>
