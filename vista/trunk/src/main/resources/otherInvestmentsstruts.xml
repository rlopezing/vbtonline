<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="OtherInvestments" namespace="/" extends="struts-default,json-default">

       <action name="OtherInvestments_cargarOtrasInversiones" method="cargarOtrasInversiones" class="ve.com.vbtonline.vista.action.otherInvestments.OtherInvestmentsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,otherInvestmentsServicio,data</param>
            </result>
        </action>

        <action name="OtherInvestments_cargarOtrasInversionesBloqueos" method="cargarOtrasInversionesBloqueos" class="ve.com.vbtonline.vista.action.otherInvestments.OtherInvestmentsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,otherInvestmentsServicio,data</param>
            </result>
        </action>

        <action name="OtherInvestments_consultarDetalleOtrasInversionesBT" method="consultarDetalleOtrasInversionesBT" class="ve.com.vbtonline.vista.action.otherInvestments.OtherInvestmentsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,otherInvestmentsServicio,data</param>
            </result>
        </action>

        <action name="OtherInvestments_consultarSaldosTransOtrasInversiones" method="consultarSaldosTransOtrasInversiones" class="ve.com.vbtonline.vista.action.otherInvestments.OtherInvestmentsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,otherInvestmentsServicio,data</param>
            </result>
        </action>

        <action name="OtherInvestments_consultarBloqueosOtrasInversiones" method="consultarBloqueosOtrasInversiones" class="ve.com.vbtonline.vista.action.otherInvestments.OtherInvestmentsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,otherInvestmentsServicio,data</param>
            </result>
        </action>

        <action name="OtherInvestments_consultarEstadoCuenta" method="consultarEstadoCuenta" class="ve.com.vbtonline.vista.action.otherInvestments.OtherInvestmentsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,otherInvestmentsServicio,data</param>
            </result>
        </action>

        <action name="OtherInvestments_cargarOtrasInversionesRazonMoneda" method="cargarOtrasInversionesRazonMoneda" class="ve.com.vbtonline.vista.action.otherInvestments.OtherInvestmentsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,otherInvestmentsServicio,data</param>
            </result>
        </action>

        <action name="OtherInvestments_generarPDFEstadoCuenta" method="generarPDFEstadoCuenta" class="ve.com.vbtonline.vista.action.otherInvestments.OtherInvestmentsAction">
            <result name="success" type="stream">
                <param name="inputName">respuestaInputStream</param>
                <param name="contentType">application/pdf</param>
                <param name="contentDisposition">attachment;filename="EstadoCuenta.pdf"</param>
                <param name="bufferSize">2048</param>
            </result>
        </action>
        <!-- Add actions here -->
    </package>
</struts>
