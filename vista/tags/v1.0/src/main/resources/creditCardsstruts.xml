<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="CreditCards" namespace="/" extends="struts-default,json-default">

        <action name="CreditCards_cargarTarjetas" method="cargarTarjetasEstadoCuentaTDC" class="ve.com.vbtonline.vista.action.creditCards.CreditCardsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,creditCardsServicio,data</param>
            </result>
        </action>
        <action name="CreditCards_cargarDetalleYMovimientosEstadoCuentaTDC" method="cargarDetalleYMovimientosEstadoCuentaTDC" class="ve.com.vbtonline.vista.action.creditCards.CreditCardsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,creditCardsServicio,data</param>
            </result>
        </action>
        <action name="CreditCards_cargarMovimientosTDCITT" method="cargarMovimientosTDCITT" class="ve.com.vbtonline.vista.action.creditCards.CreditCardsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,creditCardsServicio,data</param>
            </result>
        </action>

        <action name="CreditCards_cargarTarjetasITT" method="cargarTarjetasITT" class="ve.com.vbtonline.vista.action.creditCards.CreditCardsAction">
            <result name="success" type="json">
                <param name="excludeProperties">fabrica,creditCardsServicio,data</param>
            </result>
        </action>

        <!-- Add actions here -->
    </package>
</struts>
